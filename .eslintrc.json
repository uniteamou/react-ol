{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "@typescript-eslint/no-empty-object-type": 0,
    "no-console": ["error", { "allow": ["warn", "error"] }]
  },
  "env": {
    "jest": true
  }
}
